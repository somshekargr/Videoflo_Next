        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/admin-console-frontend-e2e/cypress.json",
            "tsConfig": "apps/admin-console-frontend-e2e/tsconfig.e2e.json",
            "devServerTarget": "admin-console-frontend:serve"
